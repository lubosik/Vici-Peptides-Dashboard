# Demo Mode (default: true)
# Set to 'false' to use real Supabase/WooCommerce integration
NEXT_PUBLIC_DEMO_MODE=true

# Supabase Configuration (only needed if DEMO_MODE=false)
# NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
# NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
# SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# WooCommerce Configuration (only needed if DEMO_MODE=false)
# WOOCOMMERCE_STORE_URL=your_store_url
# WOOCOMMERCE_CONSUMER_KEY=your_consumer_key
# WOOCOMMERCE_CONSUMER_SECRET=your_consumer_secret
